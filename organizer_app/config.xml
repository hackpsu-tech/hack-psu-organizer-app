<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.hackpsuorganizer" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>HackPSU Organizer</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="mdh5389@psu.edu">
        Matthew Heilman
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="ldpi" src="www/res/android/ldpi.png" />
        <icon density="mdpi" src="www/res/android/mdpi.png" />
        <icon density="hdpi" src="www/res/android/hdpi.png" />
        <icon density="xhdpi" src="www/res/android/xhdpi.png" />
        <icon density="xxhdpi" src="www/res/android/xxhdpi.png" />
        <icon density="xxxhdpi" src="www/res/android/xxxhdpi.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <icon height="180" src="www/res/ios/icon-60@3x.png" width="180" />
        <icon height="60" src="www/res/ios/icon-60.png" width="60" />
        <icon height="120" src="www/res/ios/icon-60@2x.png" width="120" />
        <icon height="76" src="www/res/ios/icon-76.png" width="76" />
        <icon height="152" src="www/res/ios/icon-76@2x.png" width="152" />
        <icon height="57" src="www/res/ios/icon.png" width="57" />
        <icon height="114" src="www/res/ios/icon@2x.png" width="114" />
        <icon height="72" src="www/res/ios/icon-72.png" width="72" />
        <icon height="144" src="www/res/ios/icon-72@2x.png" width="144" />
        <icon height="167" src="www/res/ios/icon-167.png" width="167" />
        <icon height="29" src="www/res/ios/icon-small.png" width="29" />
        <icon height="58" src="www/res/ios/icon-small@2x.png" width="58" />
        <icon height="50" src="www/res/ios/icon-50.png" width="50" />
        <icon height="100" src="www/res/ios/icon-50@2x.png" width="100" />
        <icon height="167" src="www/res/ios/icon-83.5@2x.png" width="167" />
    </platform>
    <preference name="SplashScreen" value="splash" />
    <preference name="SplashScreenDelay" value="1000" />
    <engine name="browser" spec="^5.0.1" />
    <engine name="android" spec="^6.3.0" />
    <plugin name="cordova-plugin-browsertab" spec="^0.2.0" />
    <plugin name="cordova-plugin-buildinfo" spec="^2.0.1" />
    <plugin name="cordova-plugin-customurlscheme" spec="^4.3.0">
        <variable name="URL_SCHEME" value="io.cordova.hackpsuorganizer" />
        <variable name="ANDROID_SCHEME" value=" " />
        <variable name="ANDROID_HOST" value=" " />
        <variable name="ANDROID_PATHPREFIX" value="/" />
    </plugin>
    <plugin name="cordova-plugin-dialogs" spec="~1.3.1" />
    <plugin name="cordova-plugin-inappbrowser" spec="^1.7.1" />
    <plugin name="cordova-plugin-qrscanner" spec="^2.5.0" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="cordova-universal-links-plugin" spec="^1.2.1" />
    <plugin name="cordova-plugin-compat" spec="^1.2.0" />
</widget>
